{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",

  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": ["${mod_authors}"],
  "contact": {},

  "license": "${mod_license}",
  "icon": "icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.chililisoup.modularsynths.fabric.ModularSynthsFabric"
    ],
    "client": [
      "dev.chililisoup.modularsynths.fabric.client.ModularSynthsClientFabric"
    ]
  },
  "mixins": [
    "modularsynths.mixins.json",
    "modularsynths-common.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": "=${minecraft_version}"
  }
}
